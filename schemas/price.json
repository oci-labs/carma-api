
            {
              "$schema": "http://json-schema.org/draft-04/schema#4",
              "title": "Price",
              "description": "Describes information about prices",
              "type": "object",
              "properties": {
                "id": {
                  "description": "unique identifier for the station with these prices",
                  "type": "string"
                },
                "prices": {
                  "description": "list of prices for the station",
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "fuel": {
                        "description": "fuel type for these prices",
                        "enum": ["regular", "premium", "diesel"]
                      }
                      "list": {
                        "description": "list of date/price pairs",
                        "type": "array",
                        "items": {
                          "description": "date/price pairs",
                          "type": "object",
                          "properties": {
                            "date": {
                              "description": "a number typed field in the data",
                              "type": "number"
                            },
                            "price": {
                              "description": "a boolean typed field in the data",
                              "type": "number"
                            }
                          },
                          "required": ["date", "price"]
                        }
                      }
                    }
                  },
                  "required": ["fuel", "list"]
                }
              },
              "required": ["id", "prices"]
            }

